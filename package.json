{
  "name": "site-builder",
  "version": "0.0.1",
  "description": "Barsema Development Site Builder",
  "private": true,
  "scripts": {
    "start": "nodemon -e scss,js,jsx -x \"npm run buildrun\"",
    "buildrun": "npm run build-css && npm run build && npm run server",
    "build-css": "node-sass --output ./dist ./src",
    "build": "webpack",
    "server": "nodemon server.js 8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbarsema/site-builder.git"
  },
  "keywords": [
    "site builder",
    "scalable front-end",
    "scalable backend"
  ],
  "author": "Matthew Barsema",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mbarsema/site-builder/issues"
  },
  "homepage": "https://github.com/mbarsema/site-builder#readme",
  "dependencies": {
    "classnames": "^2.3.1",
    "nodemon": "^2.0.7",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.1",
    "mini-css-extract-plugin": "^1.4.1",
    "node-sass": "^5.0.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0"
  }
}
